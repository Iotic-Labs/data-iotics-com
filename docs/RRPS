    rdfs:label   "Component" ;
    rdfs:comment "Component of unit";
    ## rdfs:domain  rrps:Unit or rrps:SubUnit
    rdfs:range   rrps:Component ;
    .

## -- Shape

rrps:shape-Unit
    sh:targetClass rrps:Unit ;
    sh:property
        rrps:inFleet-shape ;
    sh:property [
        sh:path rrps:contains ;
        sh:class rrps:Unit ;
    ] ;
    sh:property
        rrps:installationDate-shape ;
