    rdfs:label   "Unit in asset";
    rdfs:comment "Unit part of a customer asset" ;
    rdfs:domain  rrps:CustomerAsset ;
    rdfs:range   rrps:Unit ;
    .

## rrps:inFleet rdf:type rdf:Property ;

